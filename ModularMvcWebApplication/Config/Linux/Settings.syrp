Application
	GuestGroupId			=	1

Debug
	SuppressErrors			=	1
	DebugMode				=	0
	AutoUpdateDb			=	0

Paths
	Application
		LocalRoot				=	"/var/www/vhosts/myapp.tld/"
		PublicWebFolder			=	"Public/"
		DefaultPage				=	"LandingPage.php"
		TempFolder				=	"/var/www/vhosts/myapp.tld/Output/Temp/Prod/"
		UploadFolder			=	"Uploads/Prod/"
		GlobalEventHandlers		=	"GlobalEventHandlers.php"
		GlobalScript			=	"GlobalScript.php"
	Wafl
		WaflFolder				=	"../shared/Wafl/" //relative to LocalRoot
		DblEjFolder				=	"../shared/DblEj/" //relative to LocalRoot

Web
	DomainName				=	"myapp.tld"
	WebUrl					=	"http://myapp.tld/"
	WebUrlSecure			=	"https://myapp.tld/"
	WebUrlRelative			=	"/"
	SiteEmail				=	"noreply@myapp.tld"
	CookieLifetime			=	350000
	SiteDisplayTitle		=	New Modular Mvc Web Application
	GlobalStylesheetHasTokens	=	0